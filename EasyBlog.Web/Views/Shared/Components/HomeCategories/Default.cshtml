@model List<CategoryListDto>

<div class="col-lg-12">
    <div class="sidebar-item tags">
        <div class="sidebar-heading">
            <h2>Kategoriler</h2>
        </div>
        <div class="content">
            <ul>
                @foreach (var category in Model)
                {
                    <li><a asp-action="Search" asp-route-keyword="@category.Name">@category.Name</a></li>
                }

            </ul>
        </div>
    </div>
</div>